			import java.util.LinkedHashSet;
			import java.util.Set;
			import java.util.TreeSet;

			public class Linksetstring {
				
				// LinkedHashSet allow only one Null no duplicate value but it follows inserting order
				
				
					public static void main(String[] args) {
						
					Set<String> mylinkset =  new LinkedHashSet<String>();
					
					
					mylinkset.add(null);
					mylinkset.add("Software");
					mylinkset.add("Computer");
					mylinkset.add(null);
					mylinkset.add("Java");
					mylinkset.add("SQL");
					mylinkset.add("API");
					mylinkset.add("Manual");
					mylinkset.add(null);
					mylinkset.add("Automation");
					
					System.out.println(mylinkset);
					
					
					Set<Integer> myTreeSet = new TreeSet<Integer>();
					
					myTreeSet.add(100);
					myTreeSet.add(20);
					myTreeSet.add(800);
					myTreeSet.add(40);
					myTreeSet.add(100);
					myTreeSet.add(30);
					myTreeSet.add(200);
					//myTreeSet.add(null);
					
					
					System.out.println(myTreeSet);
					
					//It is allow Disaccending order
					
					Set<String> myTreeSet2 = new TreeSet<String>();
					//myTreeSet2.add(null);
					myTreeSet2.add("Saad");
					myTreeSet2.add("Fraz");
					myTreeSet2.add("Subhaan");
					myTreeSet2.add("Ameer");
					myTreeSet2.add("Ali");
					myTreeSet2.add("Zain");
					myTreeSet2.add("Khala");
					myTreeSet2.add("Khala");
					
					System.out.println(myTreeSet2);
					
					}
					
			}

				

		

	}

