package com.automation;



	public static void main(String[] args) {
		public class TestingMap {
			
			public static void main(String[] args) {
					
					// Key and Value = pair
					// key must be unique
					// Map does not allow any duplicate key but it allows duplicate value
					// HashMap follow the Key order ascending 
					
					Map<Integer,String>  myMap = new HashMap<Integer, String>();
					
					myMap.put(1, "Abdus");
					myMap.put(2, "Abdus");
					myMap.put(6, "Abdus");
					myMap.put(4, "Sobhan");
					myMap.put(9, "Milk");
					myMap.put(5, "Rice");
					myMap.put(7, "SQL");
				
					
					System.out.println(myMap);
					
					String obj = myMap.get(7);
					System.out.println(obj);
					
					
					  Map<String, String> myoneMap =new Hashtable<String,String>();
					  myoneMap.put("UserName", "Amina123"); 
					  myoneMap.put("FullName", "Sobhan");
					  myoneMap.put("City", "New York");
					  myoneMap.put("Country", "Sobhan"); 
					  myoneMap.put("FirstName", "Sobhan");
					  myoneMap.put("LastName", "Akter");
					  
					 String value = myoneMap.get("City");
					  System.out.println(value);
					  System.out.println(myoneMap); 
					  System.out.println(myoneMap.size());
					  
						
						  Map<String, String> mytreemap = new TreeMap<String, String>(); 
						  // TreeMap follow Ascending Key Order 
						  mytreemap.put("username","sobharstech@gmail.com"); 
						  mytreemap.put("Password", "sobhan@123");
						  mytreemap.put("Subj", "Selenium"); 
						  mytreemap.put("America", "Jackson Heights");
						  mytreemap.put("Position", "QA Lead"); 
						  mytreemap.put("Designation","QA Analyst"); 
						  mytreemap.put("Pos", "QA Lead"); 
						  mytreemap.put("Designation", "QA Analyst");
						
						  System.out.println(mytreemap);
						 
					 
				}


			}



			import java.util.HashMap;

			public class mymaplern {
				
				public static void main(String[] args) {
					
					
